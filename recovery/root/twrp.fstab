/boot		   emmc		    /dev/block/platform/mtk-msdc.0/by-name/boot         flags=display="Boot";backup=1
/system		   auto		    /dev/block/platform/mtk-msdc.0/by-name/system       flags=display="System";backup=1
/cache		   ext4		    /dev/block/platform/mtk-msdc.0/by-name/cache
/data		   ext4		    /dev/block/platform/mtk-msdc.0/by-name/userdata     flags=display="Data";backup=1;encryptable=/dev/block/platform/mtk-msdc.0/by-name/metadata
/nvdata        ext4         /dev/block/platform/mtk-msdc.0/by-name/nvdata       flags=display="NVDATA";backup=1
/nvram 		   emmc         /dev/block/platform/mtk-msdc.0/by-name/nvram        flags=display="IMEI/NVRAM";backup=1
/recovery	   emmc		    /dev/block/platform/mtk-msdc.0/by-name/recovery     flags=display="Recovery";backup=1

/external_sd  vfat          /dev/block/mmcblk1p1 /dev/block/mmcblk1             flags=display="Micro-SD";storage;wipeingui;removable
